{
  "name": "happy-barnacle",
  "version": "1.1.2",
  "description": "A backend server for persisting object events.",
  "keywords": [],
  "author": "Axel",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelkr/happy-barnacle.git"
  },
  "main": "dist/main.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist/*.js"
  ],
  "scripts": {
    "build": "tsc",
    "buildProduction": "tsc --project tsconfig.production.json",
    "start": "node dist/main.js",
    "clean": "(rm -r ./dist || true)",
    "clean:all": "npm run clean && (rm -r ./node_modules || true)",
    "prepare": "npm run build",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "testCLI": "mocha -r ts-node/register --reporter mocha-junit-reporter src/**/*.spec.ts",
    "upgrade": "npx npm-check -u",
    "lint": "eslint src/ --ext .ts"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.1",
    "@types/chai": "^4.2.14",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "chai": "^4.2.0",
    "eslint": "^7.18.0",
    "mocha": "^8.2.1",
    "mocha-junit-reporter": "^2.0.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "better-sqlite3": "^7.1.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "sitka": "^1.0.5"
  }
}
